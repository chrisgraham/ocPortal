a:223:{s:24:"CUSTOM_PRODUCT_USERGROUP";s:22:"Usergroup subscription";s:20:"CUSTOM_PRODUCT_OTHER";s:23:"Other/Misc. transaction";s:19:"CUSTOM_PRODUCT_WAGE";s:4:"Wage";s:18:"CUSTOM_PRODUCT_TAX";s:3:"Tax";s:23:"CUSTOM_PRODUCT_INTEREST";s:8:"Interest";s:19:"CUSTOM_PRODUCT_WORK";s:22:"Custom work (invoices)";s:23:"PAID_SUBSCRIPTION_ENDED";s:28:"Usergroup subscription ended";s:25:"PAID_SUBSCRIPTION_STARTED";s:30:"Usergroup subscription started";s:24:"_PAID_SUBSCRIPTION_ENDED";s:151:"Your subscription to {1} is no longer active. If you wish to re-activate your subscription, you must create a new subscription:\n\n[url="{2}"]{2}[/url]";s:26:"_PAID_SUBSCRIPTION_STARTED";s:102:"Thank you for subscribing to paid membership. Your account has now been upgraded.\n\nBest wishes, {1}.";s:23:"_PAID_SUBSCRIPTION_UHOH";s:219:"You had a paid subscription on {1}, but that subscription is no longer available. You may wish to confirm that the subscription is no longer being paid for, although measures should have already been taken to cancel it.";s:16:"MANAGE_ECOMMERCE";s:16:"Manage eCommerce";s:24:"DESCRIPTION_INVOICE_PAGE";s:190:"Use this screen to invoice member. Each invoice is tied against a specific &lsquo;product&rsquo;, be that a product implying a specific deliverable or a general one used for adhoc invoicing.";s:24:"INVOICE_REQURIRED_AMOUNT";s:103:"You added an invoice for a product that had no pre-set price, and you did not specify a price yourself.";s:11:"TRANSACTION";s:11:"Transaction";s:19:"PURCHASE_STAGE_misc";s:16:"choose a product";s:22:"PURCHASE_STAGE_message";s:18:"what you're buying";s:22:"PURCHASE_STAGE_licence";s:9:"agreement";s:22:"PURCHASE_STAGE_details";s:13:"enter details";s:18:"PURCHASE_STAGE_pay";s:12:"make payment";s:21:"PURCHASE_STAGE_finish";s:8:"finished";s:22:"NOTHING_TO_INVOICE_FOR";s:72:"There is nothing that an invoice may be made for in the eCommerce system";s:29:"MANUAL_TRANSACTION_IDENTIFIER";s:150:"The meaning of this ID depends upon the product sold