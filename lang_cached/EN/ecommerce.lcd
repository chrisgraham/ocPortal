paid";s:21:"PAYMENT_STATE_pending";s:7:"Pending";s:18:"PAYMENT_STATE_paid";s:4:"Paid";s:23:"PAYMENT_STATE_delivered";s:9:"Fulfilled";s:20:"PAYMENT_STATE_active";s:6:"Active";s:23:"PAYMENT_STATE_cancelled";s:9:"Cancelled";s:19:"SUBSCRIPTION_CANCEL";s:19:"Cancel subscription";s:9:"PER_MONTH";s:9:"per month";s:7:"PER_DAY";s:7:"per day";s:8:"PER_WEEK";s:8:"per week";s:8:"PER_YEAR";s:8:"per year";s:9:"X_PER_DAY";s:22:"every {1} {1|day|days}";s:10:"X_PER_WEEK";s:24:"every {1} {1|week|weeks}";s:11:"X_PER_MONTH";s:26:"every {1} {1|month|months}";s:10:"X_PER_YEAR";s:24:"every {1} {1|year|years}";s:19:"INVOICE_AMOUNT_TEXT";s:123:"This is the amount on the invoice, measured in {1}. Leave this blank to use the default for the product being invoiced for.";s:12:"INVOICE_NOTE";s:4:"Note";s:15:"INVOICE_SPECIAL";s:19:"Product-specific ID";s:27:"DESCRIPTION_INVOICE_SPECIAL";s:64:"Anything you like, to help you identify what the invoice is for.";s:15:"INVOICE_SUBJECT";s:25:"Invoice ready for payment";s:15:"INVOICE_MESSAGE";s:148:"An electronic invoice has been prepared for you at {2}.\n\nYou may view this, and any other invoices using the link below:\n[url="{1}"]{1}[/url]\n\n";s:8:"INVOICES";s:8:"Invoices";s:9:"CASH_FLOW";s:17:"Cash flow diagram";s:11:"PROFIT_LOSS";s:19:"Profit/Loss account";s:13:"BALANCE_SHEET";s:13:"Balance sheet";s:15:"OPENING_BALANCE";s:15:"Opening balance";s:10:"M_INTEREST";s:8:"Interest";s:15:"M_INTEREST_PLUS";s:15:"Interest gained";s:16:"M_INTEREST_MINUS";s:13:"Interest paid";s:8:"EXPENSES";s:14:"Expenses/Costs";s:16:"TRANSACTION_FEES";s:16:"Transaction fees";s:5:"WAGES";s:18:"Wages/Income-taxes";s:11:"TAX_GENERAL";s:24:"Non-sales/Non-income tax";s:15:"CLOSING_BALANCE";s:15:"Closing balance";s:10:"NET_PROFIT";s:21:"Net Profit before tax";s:7:"DELIVER";s:6:"Fulfil";s:12:"MAKE_PAYMENT";s:12:"Make payment";s:18:"START_SUBSCRIPTION";s:18:"Start subscription";s:26:"PRODUCT_NAME_SPECIAL_CHARS";s:68:"Please do not include special characters in your subscription title.