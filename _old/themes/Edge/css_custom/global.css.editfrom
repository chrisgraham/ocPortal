}

.standardbox_curved_nontitle_middle {
	border-top: 1px solid #c1cee3; /* {$,wizard, 100% lightborder} */
}

.standardbox_curved_bottom_middle {
	border-bottom: 1px solid #c1cee3; /* {$,wizard, 100% lightborder} */
}

.standardbox_curved_title_left {
	background: url("{$IMG,standardboxes/title_left}") 0 0 no-repeat;
}

.standardbox_curved_nontitle_left {
