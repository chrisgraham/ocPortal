	}
{+END}

/* Basic styling */
.standardbox_wrap_panel {
	border: 1px solid #8ba4ca; /* {$,wizard, 61% seed + 39% W/B} */
	zoom: 1; /* IE6 does not display bottom borders if this is not here */
}
/* Precedence (!important) for browsers that support the > and + selectors; precedence stops it relying on .interlock being right */
html>body .standardbox_wrap_panel { /* Runs before the below selector, the best we can to given we don't have multiple levels of important or negation in CSS; it's needed to take precedence over the .interlock selector */
	border-top: 1px solid #8ba4ca !important; /* {$,wizard, 61% seed + 39% W/B} */
	margin-top: 5px !important;
}
{+START,IF,{$NOT,{$MOBILE}}}
	.standardbox_wrap_panel + .standardbox_wrap_panel, html>body.fake_middle_continuation div.interlock {
		border-top: 0px !important;
		margin-top: 0 !important;
	}
	/* Otherwise (IE6) will use the following, which depends on Tempcode correctly tracking the template inclusion orders via .interlock */
	.interlock {
		border-top: 0px;
		margin-top: 0;
	}
{+END}

.standardbox_wrap_curved {
	text-align: left; /* fixes IE bug */
}

.standardbox_curved_ie_hack {
	line-height: 0px;
}

.standardbox_meta_classic {
	font-style: italic;
	font-size: 0.8em;
	border-bottom: 1px solid;
}

.standardbox_title_curved, .standardbox_title_curved_iesucks {
	padding-top: 2px;
	margin-top: 0;
	margin-bottom: 0;
	{+START,IF,{$MOBILE}}
		min-height: 18px;
	{+END}
	{+START,IF,{$NOT,{$MOBILE}}}
		height: 18px;
	{+END}
	border-top: 1px solid #c1cee3; /* {$,wizard, 100% lightborder} */
	border-bottom: 1px solid #c1cee3; /* {$,wizard, 100% lightborder} */
}

.standardbox_title_curved {
	margin-left: 6px;
	margin-right: 6px;
}

.standardbox_title_curved_iesucks {
}

