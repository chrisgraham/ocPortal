/*
=========================
===Overridden Elements===
=========================
*/

html, body {
	border: 0;
}

body, textarea, input, td, th {
	font-family: 'Verdana', 'Arial', 'Helvetica', sans-serif;
}

tt, kbd, samp {
	font-size: 1.25em;
	font-weight: bold;
}

abbr,acronym {
	border-bottom: 1px dotted; /* For Safari, which does not have it by default */
}

{+START,IF,{$VALUE_OPTION,html5}}
	/* Reapply default styles non-HTML5-aware browsers */
	aside,footer,header,nav,hgroup,article {
		display: block;
	}
{+END}

textarea, input {
	font-size: 1em;
}

input[type="text"]:focus, input[type="password"]:focus {
	background: url('{$IMG;,keyboard}') right center no-repeat white;
}

input[type="text"],input[type="password"],textarea,select { /* Normally a browser default, but gets inherited on some phones */
	background: white;
}

a:link, a:visited, a:hover, a:active {
	text-decoration: underline;
}
a:link, .buttonhyperlink {
	color: #123d7b; /* {$,a.link, 73% (seed sat_to 85) + 27% !W/B} */
	-webkit-transition: color 1s linear;
	-moz-transition: color 1s linear;
	-o-transition: color 1s linear;
}
a:visited {
	color: #113871; /* {$,wizard, 67% (seed sat_to 85) + 33% !W/B} */
	-webkit-transition: color 1s linear;
	-moz-transition: color 1s linear;
	-o-transition: color 1s linear;
}
a:hover, .buttonhyperlink:hover {
	-webkit-transition: color 1s linear;
	-moz-transition: color 1s linear;
	-o-transition: color 1s linear;
	color: #051225; /* {$,a.hover, 30% a.link + 70% !W/B } */
}

code {
	font-size: 1em;
}

/* An iframe screen */
.opens_below h1 {
	display: none;
}

h1 {
	font-weight: normal;
	font-family: "Georgia", "Times New Roman", "Times", serif;
	font-size: 1.9em;
	color: #325180; /* {$,wizard, 76% seed + 24% !W/B} */
	border-bottom: 1px solid #c1cee3; /* {$,wizard, 100% lightborder} */
}

h2 {
	font-size: 1.3em;
	color: #325180; /* {$,wizard, 76% seed + 24% !W/B} */
	border-bottom: 1px solid #c1cee3; /* {$,wizard, 100% lightborder} */
	font-weight: normal;
	margin-top: 1em;
}

h3 {
	font-size: 1.1em;
	color: #325180; /* {$,wizard, 76% seed + 24% !W/B} */
	border-bottom: 1px solid #c1cee3; /* {$,wizard, 100% lightborder} */
	font-weight: normal;
}

h3 a:link,
h3 a:active,
h3 a:visited,
h3 a:hover {
	text-decoration: none;
}

h4 {
	font-size: 1em;
	font-weight: bold;
}

table {
	vertical-align: top;
	font-size: 1em;
}

td, table th.de_th {
	vertical-align: top;
	color: #0d1522; /* {$,wizard, 20% seed + 80% !W/B} */
	font-size: 1em;
	font-weight: normal;
}

th, .fake_th {
	vertical-align: top;
	background-color: #a2b6d5; /* {$,th.bgcolor, 60% sb_color + 40% seed} */
	font-size: 0.85em;
	color: #313740; /* {$,wizard, 30% th.bgcolor + 70% !W/B} */
	font-weight: bold;
	text-align: center;
}

.de_th { /* {$,Allows structurally accurate but visually non-interfering markup}  */
	text-align: {!en_left};
	background-color: transparent;
	font-weight: normal;
	border: 0;
}

.de_th a {
	font-weight: normal;
}

hr {
	height: 0;
	border: 0;
	border-bottom: 2px dotted #a2b6d5; /* {$,wizard, 60% sb_color + 40% seed} */
}

img {
	display: inline;
	border: 0;
}

iframe {
	padding: 0;
	margin: 0;
	border: 0;
}

hr { /* Fixes difference with Opera */
	text-align: left;
}

input[type="text"],textarea { /* So Comcode can be typed */
	unicode-bidi: bidi-override;
	direction: ltr;
}

#htmlarea input[type="text"], #htmlarea textarea {
	unicode-bidi: normal;
	direction: {!dir};
}

form { /* For IE6 */
	margin-bottom: 0;
}

{+START,IF,{$MOBILE}}
	select {
		max-width: 100px;
	}
{+END}

/*
=========================
=====Global interface====
=========================
*/

/* {$,washed_out, 10% (seed sat_to 85) + 90% W/B} */

/* {$,lgradcolor, 67% (seed sat_to 95) + 33% FFFFFF} */ /* {$,We always want the light source coming from the top} */
/* {$,dgradcolor, 95% (seed sat_to 65) + 5% 000000} */ /* {$,We always want the shadows at the bottom} */
/* {$,bordcolor, 60% (seed sat_to 93) + 40% !W/B} */
/* {$,lightborder, 33% seed + 67% W/B} */
/* {$,bgcol, 15% seed + 85% W/B} */
/* {$,ltgradcolor, 67% (seed sat_to 135) + 33% FFFFFF} */  /* {$,We always want the light source coming from the top} */
/* {$,dtgradcolor, 95% (seed sat_to 25) + 5% 000000} */ /* {$,We always want the shadows at the bottom} */
/* {$,sb_color, 15% seed + 85% W/B} */

/* {$,ocf_row2, 11% seed + 89% W/B}*/

/* {$,comcode_quote_left, 48% comcode_quote_right + 52% W/B} */
/* {$,comcode_quote_right, 100% comcode_code_h4} */

/* {$,zg_peak,90% zg_top + 10% W/B} */
/* {$,zg_top,48% (seed sat_to 98) + 52% FFFFFF} */
/* {$,zg_bot,97% (seed sat_to 71) + 3% 000000} */

/* {$,medborder.border, 95% (seed sat_to 33) + 5% !W/B} */

/* {$,ocfpostindicator, 65% (seed sat_to 100) + 35% W/B} */
/* {$,ocfredirectindicator, 65% (seed sat_to 100) + 35% W/B + 000038 - 061600} */

/* {$,a.link__dark, 63% (seed sat_to 85) + 37% !W/B} */
/* {$,a.hover__dark, 20% a.link + 80% !W/B } */

/* {$,special_borderer, 76% seed + 24% !W/B} */
/* {$,special_middle, 45% (seed sat_to 85) + 65% !W/B} */

.re_body, #htmlarea {
	margin: 0;
	font-size: 0.8em;
}

/* Consider .re_body as 'body': it is just we cannot put our styles directly on body because sometimes this gets used in things we should not mess with such as the WYSIWYG editors frame (which is #htmlarea) and e-mails */
.re_body {
	padding: 0 !important;
	color: #0d1522; /* {$,wizard, 20% seed + 80% !W/B} */
	background: #FFFFFF; /* {$,wizard, 100% W/B} */
}

